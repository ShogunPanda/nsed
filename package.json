{
  "name": "nsed",
  "version": "3.0.9",
  "description": "Javascript Terminal String Editor",
  "homepage": "https://sw.cowtech.it/nsed",
  "repository": "github:ShogunPanda/nsed",
  "keywords": [
    "terminal",
    "string",
    "editor"
  ],
  "bugs": {
    "url": "https://github.com/ShogunPanda/nsed/issues"
  },
  "author": "Shogun <shogun@cowtech.it>",
  "license": "ISC",
  "private": false,
  "files": [
    "bin",
    "dist",
    "types",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "bin": {
    "nsed": "bin/nsed.mjs"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "typings": "./types/index.d.ts",
  "types": "./types/index.d.ts",
  "scripts": {
    "dev": "swc -s -w -d dist src",
    "prebuild": "rm -rf dist types && npm run typecheck && npm run lint",
    "build": "swc --delete-dir-on-start -d dist src",
    "format": "prettier -w src test",
    "typecheck": "tsc -p . --emitDeclarationOnly",
    "lint": "eslint src test",
    "test": "c8 -c test/config/c8-local.json tap --rcfile=test/config/tap.yml test/*.test.ts",
    "test:ci": "c8 -c test/config/c8-ci.json tap --rcfile=test/config/tap.yml --no-color test/*.test.ts",
    "ci": "npm run build && npm run test:ci",
    "prepublishOnly": "npm run ci",
    "postpublish": "git push origin && git push origin -f --tags"
  },
  "dependencies": {
    "commander": "^9.4.0",
    "get-stream": "^6.0.1",
    "pump": "^3.0.0",
    "split2": "^4.1.0"
  },
  "devDependencies": {
    "@cowtech/eslint-config": "^8.7.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.244",
    "@types/node": "^18.7.13",
    "@types/pump": "^1.1.1",
    "@types/sinon": "^10.0.13",
    "@types/split2": "^3.2.1",
    "@types/tap": "^15.0.7",
    "c8": "^7.12.0",
    "chokidar": "^3.5.3",
    "prettier": "^2.7.1",
    "sinon": "^14.0.0",
    "tap": "^16.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
