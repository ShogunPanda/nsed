{
  "name": "nsed",
  "version": "1.0.1",
  "description": "Javascript Terminal String Editor",
  "main": "index.js",
  "files": [
    "index.js",
    "lib",
    "README.md",
    "CHANGELOG.md"
  ],
  "bin": {
    "nsed": "./bin/nsed"
  },
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "test-watch": "./node_modules/.bin/jest --watch",
    "lint": "./node_modules/.bin/eslint index.js bin lib test",
    "sanity": "npm run lint && npm run coverage",
    "cleanup": "rm -rf coverage",
    "ci": "npm test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postpublish": "git push origin && git push origin -f --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShogunPanda/nsed"
  },
  "keywords": [],
  "author": "Shogun <shogun@cowtech.it>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShogunPanda/nsed/issues"
  },
  "homepage": "https://sw.cowtech.it/nsed",
  "engines": {
    "node": ">=7.6.0"
  },
  "dependencies": {
    "byline": "~5.0",
    "commander": "~2.9",
    "date-fns": "~1.28",
    "date-fns-strftime": "~1.0",
    "get-stdin": "~5.0",
    "voca": "~1.3"
  },
  "devDependencies": {
    "coveralls": "^2.13.0",
    "eslint": "~> 3.18",
    "eslint-config-cowtech": "~> 3.18",
    "jest": "~19.0"
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/bin",
      "/test"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  }
}
